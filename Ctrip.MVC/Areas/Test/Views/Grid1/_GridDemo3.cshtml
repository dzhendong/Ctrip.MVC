@using Ctrip.Component
@using Ctrip.Model
@model Ctrip.Model.OrderListModelV2
@{
    if (Model == null)
    {
        return;
    }

    var pageNo = 1;
    pageNo = (Request.QueryString["pageNo"] == null || Request.QueryString["pageNo"] == string.Empty) ? 1 :
        int.Parse(Request.QueryString["pageNo"].ToString());
}
<div id="myGrid3">
    <div id="Table3">
        @Html.Table(Model.columns, Model.options, Model.orders)
    </div>
    <div id="Pager3" style="margin-bottom: 20px; margin-top: 20px; width:800px;">
        @Html.AjaxPager(567, 10, pageNo, "pageNo", "myGrid3",
            new PagerConfig()
            {
                ShowNumericPagerItems = false,
                ShowPageIndexBox = false,
                ShowFirstLast = false,
                HtmlAttributes = new { @style = "text-align:right;" }
            })
    </div>
</div>
